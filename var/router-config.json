{
  "routingDefaults": {
    "itineraryFilters": {
      "accessibilityScore": true,
      "removeItinerariesWithSameRoutesAndStops": "${REMOVE_ITINERARIES_WITH_SAME_ROUTES_AND_STOPS}"
    },
    "searchWindow": "${SEARCH_WINDOW}",
    "transferSlack": 90,
    "waitReluctance": 3.25,
    "transitReluctanceForMode": {
      "BUS": 1.5,
      "SUBWAY": 0.65,
      "RAIL": 0.95
    },
    "walk": {
      "speed": 1.3,
      "reluctance": 4.0,
      "stairsReluctance": 1.5,
      "stairsTimeFactor": 2,
      "boardCost": 120
    },
    "wheelchairAccessibility": {
      "trip": {
        "onlyConsiderAccessible": false,
        "unknownCost": 600,
        "inaccessibleCost": 3600
      },
      "stop": {
        "onlyConsiderAccessible": false,
        "unknownCost": 600,
        "inaccessibleCost": 3600
      },
      "elevator": {
        "onlyConsiderAccessible": false
      },
      "inaccessibleStreetReluctance": 25,
      "maxSlope": 0.08333,
      "slopeExceededReluctance": 50,
      "stairsReluctance": 25
    }
  },
  "transit": {
    "maxSearchWindow": "${MAX_SEARCH_WINDOW}"
  },
  "updaters": [
    {
      "type": "real-time-alerts",
      "frequency": "100s",
      "url": "${URL_ALERTS}",
      "feedId": "mbta-ma-us"
    },
    {
      "type": "stop-time-updater",
      "frequency": "10s",
      "url": "${URL_TRIP_UPDATES}",
      "feedId": "mbta-ma-us"
    },
    {
      "type": "vehicle-positions",
      "frequency": "1m",
      "url": "${URL_VEHICLE_POSITIONS}",
      "feedId": "mbta-ma-us"
    }
  ]
}
