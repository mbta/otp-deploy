{
  "routingDefaults": {
    "itineraryFilters": {
      "accessibilityScore": true,
      "removeItinerariesWithSameRoutesAndStops": "${REMOVE_ITINERARIES_WITH_SAME_ROUTES_AND_STOPS}"
    },
    "searchWindow": "${SEARCH_WINDOW}"
  },
  "transit": {
    "maxSearchWindow": "${MAX_SEARCH_WINDOW}"
  },
  "updaters": [
    {
      "type": "real-time-alerts",
      "frequency": "100s",
      "url": "${UPDATE_URL}Alerts.pb",
      "feedId": "mbta-ma-us"
    },
    {
      "type": "stop-time-updater",
      "frequency": "10s",
      "url": "${UPDATE_URL}TripUpdates.pb",
      "feedId": "mbta-ma-us"
    },
    {
      "type": "vehicle-positions",
      "frequency": "1m",
      "url": "${UPDATE_URL}VehiclePositions.pb",
      "feedId": "mbta-ma-us"
    }
  ]
}
